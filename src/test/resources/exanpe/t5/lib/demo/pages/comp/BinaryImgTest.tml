<t:layout xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter"
	xmlns:ex="tapestry-library:exanpe">
  
  	From file : 
  	<ex:binaryImg inputStream="filestream" contentType="image/jpeg" style="border:1px solid red"/>
  
    <br/><hr/><br/>
  	
  	From byte array : 
  	<ex:binaryImg inputStream="bytestream" contentType="image/png" style="border:1px solid red"/>
  
    <br/><hr/><br/>
  	   
  	Error : 
  	<ex:binaryImg inputStream="errorstream" contentType="image/png" style="border:1px solid red"/>
  
    <br/><hr/><br/>
</t:layout>