<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" 
			 xmlns:p="tapestry:parameter">
	
	<div id="exanpe-gmap-${clientId}" class="exanpe-gmap-map"></div>
	<div class="exanpe-gmap-infos">
		<t:body/>		
		<ul>
			<t:loop source="model.items" value="current">
			  	<li id="${current.id}">
			  		<a href="#">
						${current.title}
					</a>
				</li>
			</t:loop>
		</ul>
	</div>
	<div class="cleared" />
</t:container>