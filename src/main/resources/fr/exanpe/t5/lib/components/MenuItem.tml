<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<t:container
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
	xmlns:p="tapestry:parameter">

	<t:block id="barItemRender">
		 <li id="${clientId}" class="yuimenubaritem exanpe-menubaritem ${disabledClass}">
		 	${label }
		 	<t:body/>
		 </li>
	</t:block>

	
	<t:block id="itemRender">
		<li id="${clientId}" class="yuimenuitem exanpe-menuitem ${disabledClass}">
			${label }
			<t:body/>
		</li>
	</t:block>

	<t:delegate to="rendered"/>
    	
</t:container>